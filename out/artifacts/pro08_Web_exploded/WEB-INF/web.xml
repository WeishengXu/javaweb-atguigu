<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Demo06Servlet</servlet-name>
        <servlet-class>com.atguigu.servlet.Demo06Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Demo07Servlet</servlet-name>
        <servlet-class>com.atguigu.servlet.Demo07Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Demo06Servlet</servlet-name>
        <url-pattern>/demo06</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Demo07Servlet</servlet-name>
        <url-pattern>/demo07</url-pattern>
    </servlet-mapping>
</web-app>